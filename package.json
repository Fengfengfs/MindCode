{
  "name": "code-mind",
  "displayName": "CodeMind",
  "version": "0.1.21",
  "packageManager": "pnpm@7.0.1",
  "description": "AI 生成",
  "keywords": [],
  "homepage": "https://github.com/Fengfengfs/MindCode",
  "bugs": {
    "url": "https://github.com/Fengfengfs/MindCode/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Fengfengfs/MindCode"
  },
  "main": "./dist/index.js",
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "engines": {
    "vscode": "^1.67.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "*",
		"onCommand:extension.helloWorld",
		"onCommand:extension.ttShowWindow",
		"onView:data"
  ],
  "icon": "res/icon-dark.png",
  "scripts": {
    "build": "tsup src/index.ts --external vscode",
    "dev": "nr build --watch",
    "lint": "eslint .",
    "vscode:prepublish": "nr build",
    "publish": "vsce publish --no-dependencies --allow-star-activation",
    "pack": "vsce package --no-dependencies --allow-star-activation",
    "test": "vitest",
    "typecheck": "tsc --noEmit",
    "release": "bumpp --commit --push --tag && nr publish"
  },
  "contributes": {
    "commands": [
			{
				"command": "ftpExplorer.refresh",
				"title": "Refresh",
				"icon": {
					"light": "resources/light/refresh.svg",
					"dark": "resources/dark/refresh.svg"
				}
			},
			{
				"command": "ftpExplorer.openFtpResource",
				"title": "Open FTP Resource"
			},
			{
				"command": "ftpExplorer.revealResource",
				"title": "Reveal in FTP View"
			},
      {
        "command": "Mind-code.createFileFromTemplate",
        "title": "Create File From Template"
      },
      {
        "command": "Mind-code.requirement",
        "title": "input requirement"
      },
			{
				"command": "extension.ttShowWindow",
				"title": "Tt Show Window"
			},
			{
				"command": "data.refresh",
				"title": "refresh"
			},
			{
				"command": "data.showme",
				"title": "AI 分析"
			},
      {
				"command": "data.generate",
				"title": "AI 生成"
			},
      {
				"command": "data.time",
				"title": "AI TIME"
			},
			{
				"command": "data.create",
				"title": "create"
			}
    ],
		"viewsContainers": {
			"activitybar": [
				{
					"id": "tticon",
					"icon": "media/dep.png",
					"title": "tt icon"
				}
			]
		},
		"views": {
			"tticon": [
				{
					"id": "data",
					"name": "data"
				}
			],
      "explorer": [
				{
					"id": "ftpExplorer",
					"name": "FTP Explorer"
				}
      ]
		},
    "menus": {
			"commandPalette": [
				{
					"command": "ftpExplorer.revealResource"
				}
			],
			"view/title": [
				{
					"command": "data.create",
					"group": "1_modification"
				},
				{
					"command": "ftpExplorer.refresh",
					"when": "view == ftpExplorer",
					"group": "navigation"
				}
			],
			"view/item/context":[
				{ 
					"command": "data.showme",
					"when": "view == data" ,
					"group": "inline"
				},
        	{ 
					"command": "data.generate",
					"when": "view == data" ,
					"group": "inline"
				},
        	{ 
					"command": "data.time",
					"when": "view == data" ,
					"group": "inline"
				}
			],
      "explorer/context": [
        {
          "command": "Mind-code.createFileFromTemplate",
          "group": "1_modification"
        },
        {
          "command": "Mind-code.requirement",
          "group": "1_modification"
        }
      ]
    },
    "configuration": {
      "title": "test Mind Code",
      "properties": {
        "goldRight.templateDirectoryPath": {
          "type": "string",
          "description": "Template directory path."
        }
      }
    }
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.23.0",
    "@antfu/ni": "^0.16.2",
    "@babel/types": "^7.17.10",
    "@types/node": "^17.0.32",
    "@types/vscode": "^1.67.0",
    "bumpp": "^7.1.1",
    "eslint": "^8.15.0",
    "esno": "^0.14.1",
    "pnpm": "^7.0.1",
    "rimraf": "^3.0.2",
    "tsup": "^5.12.1",
    "typescript": "^4.6.4",
    "vite": "^2.9.8",
    "vitest": "^0.12.4",
    "vsce": "^2.9.1"
  },
  "dependencies": {
    "axios": "^1.4.0",
    "rxjs": "^7.3.0"

  }
}
